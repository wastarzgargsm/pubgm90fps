Help()
{
              echo -e "\e[32mğ“ğ¡ğ¢ğ¬ ğ­ğ¨ğ¨ğ¥ ğ¦ğšğ¤ğ¢ğ§ğ  ğ©ğ®ğ›ğ  \e[3m90\e[m ğŸğ©ğ¬ \e[31m\e[4mversion\e[2m(1.4)\e[m" >&2
              echo
              echo -e "\e[31mğ’ğ²ğ§ğ­ğğ±\e[m \e[34m:\e[m \e[33mğ“ğğ¦ğ©ğ¥ğšğ­ğğ¬ \e[m[-g|h|x|v]"
              echo -e "\e[45mOptions\e[m :"
              echo
              echo -e "\e[36m-ğ  , --ğ ğŸğ± ,ğ ğŸğ±             ğŸğ¨ğ« ğ ğŸğ± ğ­ğ¨ğ¨ğ¥\e[m"
              echo -e "\e[36m-ğ¡ , --ğ¡ğğ¥ğ© , ğ¡ğğ¥ğ©     ğŸğ¨ğ« ğ¡ğğ¥ğ©\e[m"
              echo -e "\e[36m-ğ± , --ğ ğ¢ğ­ , ğ ğ¢ğ­              ğŸğ¨ğ« ğ ğ¢ğ­ğ¡ğ®ğ›\e[m"
              echo -e "\e[36m-ğ¯ , --ğ¯ğğ«ğ¬ğ¢ğ¨ğ§ , ğ¯ğğ«ğ¬ğ¢ğ¨ğ§ ğŸğ¨ğ« ğ¯ğğ«ğ¬ğ¢ğ¨ğ§\e[m"
              echo
}
Help
gfx()
{
spinner() {
echo -e '
               \e[33mâ”¬  â”Œâ”€â”â”Œâ”€â”â”Œâ”¬â”â”¬â”Œâ”â”Œâ”Œâ”€â”
               \e[33mâ”‚  â”‚ â”‚â”œâ”€â”¤ â”‚â”‚â”‚â”‚â”‚â”‚â”‚ â”¬
               \e[33mâ”´â”€â”˜â””â”€â”˜â”´ â”´â”€â”´â”˜â”´â”˜â””â”˜â””â”€â”˜\e[m
'
progreSh() {
    LR='\033[1;31m'
    LG='\033[1;32m'
    LY='\033[1;33m'
    LC='\033[1;36m'
    LW='\033[1;37m'
    NC='\033[0m'
    if [ "${1}" = "0" ]; then TME=$(date +"%s"); fi
    SEC=`printf "%04d\n" $(($(date +"%s")-${TME}))`; SEC="$SEC sec"
    PRC=`printf "%.0f" ${1}`
    SHW=`printf "%3d\n" ${PRC}`
    LNE=`printf "%.0f" $((${PRC}/2))`
    LRR=`printf "%.0f" $((${PRC}/2-12))`; if [ ${LRR} -le 0 ]; then LRR=0; fi;
    LYY=`printf "%.0f" $((${PRC}/2-24))`; if [ ${LYY} -le 0 ]; then LYY=0; fi;
    LCC=`printf "%.0f" $((${PRC}/2-36))`; if [ ${LCC} -le 0 ]; then LCC=0; fi;
    LGG=`printf "%.0f" $((${PRC}/2-48))`; if [ ${LGG} -le 0 ]; then LGG=0; fi;
    LRR_=""
    LYY_=""
    LCC_=""
    LGG_=""
    for ((i=1;i<=13;i++))
    do
    	DOTS=""; for ((ii=${i};ii<13;ii++)); do DOTS="${DOTS}."; done
    	if [ ${i} -le ${LNE} ]; then LRR_="${LRR_}â–ˆ"; else LRR_="${LRR_}."; fi
    	echo -ne "  ${LW}${SEC}  ${LR}${LRR_}${DOTS}${LY}............${LC}............${LG}............ ${SHW}%${NC}\r"
    	if [ ${LNE} -ge 1 ]; then sleep .025; fi
    done
    for ((i=14;i<=25;i++))
    do
    	DOTS=""; for ((ii=${i};ii<25;ii++)); do DOTS="${DOTS}."; done
    	if [ ${i} -le ${LNE} ]; then LYY_="${LYY_}â–ˆ"; else LYY_="${LYY_}."; fi
    	echo -ne "  ${LW}${SEC}  ${LR}${LRR_}${LY}${LYY_}${DOTS}${LC}............${LG}............ ${SHW}%${NC}\r"
    	if [ ${LNE} -ge 14 ]; then sleep .030; fi
    done
    for ((i=26;i<=37;i++))
    do
    	DOTS=""; for ((ii=${i};ii<37;ii++)); do DOTS="${DOTS}."; done
    	if [ ${i} -le ${LNE} ]; then LCC_="${LCC_}â–ˆ"; else LCC_="${LCC_}."; fi
    	echo -ne "  ${LW}${SEC}  ${LR}${LRR_}${LY}${LYY_}${LC}${LCC_}${DOTS}${LG}............ ${SHW}%${NC}\r"
    	if [ ${LNE} -ge 26 ]; then sleep .025; fi
    done
    for ((i=38;i<=49;i++))
    do
    	DOTS=""; for ((ii=${i};ii<49;ii++)); do DOTS="${DOTS}."; done
    	if [ ${i} -le ${LNE} ]; then LGG_="${LGG_}â–ˆ"; else LGG_="${LGG_}."; fi
    	echo -ne "  ${LW}${SEC}  ${LR}${LRR_}${LY}${LYY_}${LC}${LCC_}${LG}${LGG_}${DOTS} ${SHW}%${NC}\r"
    	if [ ${LNE} -ge 38 ]; then sleep .025; fi
    done
}

printf "\n\n\n\n\n\n\n\n\n\n"
progreSh 0
progreSh 10
progreSh 20
progreSh 30
progreSh 40
progreSh 50
progreSh 70
progreSh 100
printf "\n\n\n\n\n\n\n\n\n\n"

sleep 1
clear
}
