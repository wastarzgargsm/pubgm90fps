git()
{
clear
printf "\n\n\n\n\n\n"
echo opening
xdg-open https://github.com/hk4crprasad
printf "\n\n\n\n\n\n"
}
version()
{
printf "\n\n\n\n\n\n"
clear
echo -e "  ğ•ğ•–ğ•£ğ•¤ğ•šğ• ğ•Ÿ \e[31m1.2\e[m"
printf "\n\n\n\n\n\n"
}
while [[ "$#" -gt 0 ]]; do
    case $1 in
        -h|--help|help) echo; shift ;;
        -g|gfx|--gfx) gfx; shift ;;
        -x|--git|git) clear; git; shift ;;
        -v|--version|version) clear; version; shift ;;
        # ... (same format for other required arguments)
        *) echo "ğ‘¼ğ’ğ’Œğ’ğ’ğ’˜ğ’ ğ’–ğ’”ğ’‚ğ’ˆğ’† : $1" ;;
    esac
    shift
done
